<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">


<generatorConfiguration>
	<!-- classPathEntry:数据库的JDBC驱动 -->
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<!-- 去除自动生成的注释 -->
		<plugin type="org.mybatis.generator.plugins.MySQLPaginationPlugin" >
			<property name="pageClassPath" value="com.lkl.sp.db.datasource.Page"/>
		</plugin>
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
		<commentGenerator>
			<property name="suppressAllComments" value="false" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/spdb" userId="root" password="sunwell"> </jdbcConnection> 
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- targetProject:自动生成代码的位置 -->
		<javaModelGenerator targetPackage="com.lkl.sp.db.dc.mapping.gen.bean" targetProject="">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>


		<sqlMapGenerator targetPackage="com.lkl.sp.db.dc.mapping.gen" targetProject="">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>


		<javaClientGenerator type="ANNOTATEDMAPPER" targetPackage="com.lkl.sp.db.dc.mapping.gen" targetProject="">
			<property name="enableSubPackages" value="true" />
			<property name="exampleMethodVisibility" value="false" />
		</javaClientGenerator>
		<!-- tableName:用于自动生成代码的数据库表；domainObjectName:对应于数据库表的javaBean类名 -->

		
		 		 
		<table schema="" tableName="sp_user" domainObjectName="SpUser"></table>
		<table schema="" tableName="sp_order" domainObjectName="SpOrder"></table>
		<table schema="" tableName="sp_payment" domainObjectName="SpPayment"></table>
	</context>

</generatorConfiguration>
